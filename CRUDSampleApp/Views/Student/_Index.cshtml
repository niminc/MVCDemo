@model IEnumerable<CRUDSampleAppEntity.tblStudent>


<table class="table table-bordered table-condensed">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Middle Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Contact
        </th>
        <th>
            Create Date
        </th>
        <th>
            Modified Date
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MiddleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tblContactDetail.ContactDetail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifiedDate)
            </td>
            <td>
                <input type="button" value="Edit" class="btn btn-primary" onclick="Manage('@item.StudentId')" />
                <input type="button" value="Delete" class="btn btn-danger" onclick="DeleteStudent('@item.StudentId')" />
            </td>
        </tr>
    }

</table>
